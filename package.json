  "dependencies": {
    "gatsby": "^2.1.2",
    "gatsby-link": "^2.0.10",
    "gatsby-plugin-i18n": "^1.0.1",
    "gatsby-plugin-react-helmet": "^3.0.6",
    "gatsby-plugin-typescript": "^2.0.8",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-helmet": "^5.2.0",
